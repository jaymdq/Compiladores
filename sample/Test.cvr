entero indice;
arr[0..9] vector de entero;
fibo[1..10] vector de entero_lss;
suc[1..10] vector de entero_lss;

indice := 0;

iterar {
  si (indice - indice/2 * 2 = 0) entonces	** Si es par
    arr[indice] := 1;
  sino { 					** Si es impar
    arr[indice] := 0;
    imprimir('Es impar');
  }
  indice := indice + 1;
} hasta indice = 10;

indice := 3;
fibo[1] := 1;
fibo[2] := 1;
suc[1] := 1;
suc[2] := 1;

iterar {
  suc[indice] := suc[indice-2] + suc[indice-1] * suc[indice-2];
  fibo[indice] := fibo[indice-2] + fibo[indice-1];
  indice := indice + 1;
} hasta indice = 11;

